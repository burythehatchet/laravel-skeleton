{
    "name": "werxe/skeleton-laravel",
    "description": "Skeleton repository for all Laravel packages created by Werxe.",
    "keywords": [
        "php",
        "werxe"
    ],
    "homepage": "https://werxe.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Werxe",
            "email": "hello@werxe.com",
            "homepage": "https://werxe.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.6.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Werxe\\Skeleton\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Werxe\\Skeleton\\Laravel\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "component": "package",
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Werxe\\Skeleton\\Laravel\\SkeletonServiceProvider"
            ],
            "aliases": {
                "Skeleton": "Werxe\\Skeleton\\Laravel\\Facades\\Skeleton"
            }
        }
    },
    "scripts": {
        "cs:check": "./vendor/bin/php-cs-fixer fix --verbose --ansi --show-progress=estimating --diff --dry-run",
        "cs:fix": "./vendor/bin/php-cs-fixer fix --verbose --ansi --show-progress=estimating",
        "test": "./vendor/bin/phpunit --verbose --colors=always"
    },
    "minimum-stability": "stable"
}
